[MASTER]
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-continuation,
    bad-option-value,
    duplicate-code,
    invalid-name,
    locally-disabled,
    no-else-return,
    too-few-public-methods,

[REPORTS]
reports = no
score = no
msg-template = {path}:{line}: {C}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

