[MESSAGES CONTROL]
disable =
    bad-continuation,
    duplicate-code,
    invalid-name,
    locally-disabled,
    no-else-return,
    too-few-public-methods,

[REPORTS]
reports = no
msg-template = {C}: {path}:{line}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

